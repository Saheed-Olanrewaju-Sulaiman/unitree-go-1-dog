<robot name="UnitreeDog">
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://uniteedog_description/meshes/trunk.dae" scale="1.0 1.0 1.0"/>
        <!-- <box size="0.5 0.3 0.2"/> length x width x height in meters -->
      </geometry>
       <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

    <link name="hip">
    <visual>
      <origin xyz="0 0 0" rpy="0 3.1416 0"/>
      <geometry>
        <mesh filename="package://uniteedog_description/meshes/hip.dae" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="base_to_hip" type="continuous">
    <axis xyz="1 0 0"/>
    <parent link="base_link"/>
    <child link="hip"/>
    <origin xyz="-0.19 0 0" rpy="0 0 0"/>
     <limit lower="0.0" upper="0.3" effort="10" velocity="0.5"/>
  </joint>


<!-- depth Camera -->

  <link name="camera">
    <visual>
      <origin xyz="0 -0.01 0.01" rpy="0.7854 0 0"/>
      <geometry>
        <mesh filename="package://uniteedog_description/meshes/depthCamera.dae" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.9 0.29 0.0 1"/>
      </material>
    </visual>
  </link>

  <joint name="base_to_camera" type="fixed">
    
    <parent link="base_link"/>
    <child link="camera"/>
    <origin xyz="0.3 0 0" rpy="0 0 0"/>
     <!-- <limit lower="0.0" upper="0.3" effort="10" velocity="0.5"/> -->
  </joint>

  <!-- ultraSound -->

  <link name="ultraSound">
    <visual>
      <origin xyz="0 0 0.02" rpy="-0.7854 0 0"/>
      <geometry>
        <mesh filename="package://uniteedog_description/meshes/ultraSound.dae" scale="1.0 1.0 1.0"/>
      </geometry>
      <material name="base_color">
        <color rgba="0.9 0.29 0.0 1"/>
      </material>
    </visual>
  </link>

  <joint name="base_to_ultraSound" type="fixed">
    
    <parent link="camera"/>
    <child link="ultraSound"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
     <!-- <limit lower="0.0" upper="0.3" effort="10" velocity="0.5"/> -->
  </joint>

  <!--  _______________________________  -->


<!--  front leg  -->

