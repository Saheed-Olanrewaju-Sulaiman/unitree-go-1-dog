      <material name="base_color">
        <color rgba="0.59 0.29 0.0 1.0"/>
      </material>
    </visual>
  </link>

  <joint name="base_to_right_front_leg" type="fixed">
    
    <parent link="base_link"/>
    <child link="right_front_leg"/>
